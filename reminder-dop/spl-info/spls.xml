<?xml version="1.0" encoding="UTF-8"?>
<spls xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../bin/spls.xsd">
	<spl name="Reminder">
		<features>
			<feature name ="ManageReminder" />
			<feature name ="ReminderCategory" />
			<feature name ="ManageCategory" />
			<feature name ="StaticCategory" />
			<feature name ="Priority" />
			<feature name ="ReminderSchedule" />
			<feature name ="DateRange" />
			<feature name ="FixedDate" />
			<feature name ="GoogleCalendar" />
			<feature name ="Search" />
		</features>
		<configurations>
			<configuration formula="ManageReminder AND ReminderSchedule OR ReminderCategory OR Priority OR GoogleCalendar OR Search
			" />
			<configuration formula="ReminderSchedule AND (DateRange XOR FixedDate)
			" />
			<configuration formula="ReminderCategory AND (ManageCategory XOR StaticCategory)
			" />
		</configurations>
		<deltas>
			<partition>
				<modul delta="dManageReminder" when="ManageReminder AND FixedDate
				" />
			</partition>
			<partition>
				<modul delta="dManageReminder" when="ManageReminder AND DateRange
				" />
				<modul delta="dDateRange" when="ManageReminder AND DateRange
				" />
			</partition>
			<partition>
				<modul delta="dPriority" when="Priority
				" />
			</partition>
			<partition>
				<modul delta="dStaticCategory" when="StaticCategory
				" />
			</partition>
			<partition>
				<modul delta="dStaticCategory" when="ManageCategory
				" />
				<modul delta="dManageCategory" when="ManageCategory
				" />
			</partition>
			<partition>
				<modul delta="dSearch" when="Search
				" />
			</partition>
			<partition>
				<modul delta="dGoogleCalendar" when="GoogleCalendar
				" />
			</partition>
			<partition>
				<modul delta="dStaticCategoryPriority" when="Priority AND StaticCategory
				" />
			</partition>
			<partition>
				<modul delta="dManageCategoryPriority" when="Priority AND ManageCategory
				" />
			</partition>
			<partition>
				<modul delta="dDateRangePriority" when="DateRange AND Priority
				" />
			</partition>
			<partition>
				<modul delta="dDateRangeManageCategory" when="DateRange AND ManageCategory
				" />
			</partition>
			<partition>
				<modul delta="dDateRangeGoogleCalendar" when="DateRange AND GoogleCalendar
				" />
			</partition>
			<partition>
				<modul delta="dDateRangePriorityManageCategory" when="DateRange AND Priority AND ManageCategory
				" />
			</partition>
			<partition>
				<modul delta="dDateRangeSearch" when="DateRange AND Search
				" />
			</partition>
		</deltas>
	</spl>
</spls>
