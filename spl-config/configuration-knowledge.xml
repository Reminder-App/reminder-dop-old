<configurationModel>
    <configuration>
        <expression>Reminder App</expression>
        <transformation>
            <name>selectAllComponents</name>
	     <args>-</args>
        </transformation>
	<transformation>
            <name>preprocessFiles</name>
            <args>AndroidManifest, ReminderMainActivity, ReminderAdd, Strings</args>
        </transformation>
    </configuration>
    <configuration>
      <expression>StaticCategory</expression>
        <transformation>
          <name>createBuildEntries</name>
          <args>staticCategory</args>
        </transformation>
    </configuration>
    <configuration>
      <expression>Not(Static Category)</expression>
        <transformation>
          <name>removeComponents</name>
          <args>CategoriesList, CategoryRow</args>
        </transformation>
    </configuration>
</configurationModel>
